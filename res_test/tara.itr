defrule type(model)
$node...[$NL]\
endrule

defrule type(node) trigger(node)
$type+FirstUppercase[($parameter...[, ])][ $name][ > $facet...[; ]]
$node...[$NL]\
endrule

defrule type(facet) trigger(facet)
is $type+FirstUppercase[($parameter...[, ])]
endrule

defrule type(string) trigger(parameter)
'$value'
endrule