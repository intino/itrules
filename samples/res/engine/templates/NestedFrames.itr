defrule type(Roster)
<roster>
    $Coach
	<players>
		$Player...[$NL]
    </players>
</roster>
endrule

defrule type(org.siani.itrules.samples.Person) trigger(Coach)
<coach name="$Name" year="$Birthday+Year" country="$Country"/>
endrule

defrule type(org.siani.itrules.samples.Person) trigger(Player)
<player name="$Name" year="$Birthday+Year" country="$Country"/>
endrule