def type(roster)
    <roster>
        $Coach
        <players>
            $Player...[$NL]
        </players>
    </roster>
end

def type(person) trigger(coach)
    <coach name="$Name" year="$Birthday+[f-> f.isBefore(2000, ChronoUnit.Year)? "Viejo": "Joven"]" country="$Country" />
end

def type(person) trigger(player)
    <player name="$Name" year="$Birthday+Year" country="$Country"[ club="$Club"]/>
end
