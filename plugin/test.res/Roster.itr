def type(roster)
    <roster>
        $Coach
        <players>
            $Player...[$NL]
        </players>
    </roster>
end

def type(person) trigger(coach)
    <coach name="$Name" year="$Birthday+Year" country="$Country" />
end

def type(person) trigger(player)
    <player name="$Name" year="$Birthday+Year" country="$Country"[ club="$Club"]/>
end
