def type(Roster)
	<roster>
		$Coach
		<players>
			$Players...[$NL]
		</players>
    </roster>
end

def type(Person) AND trigger(Coach)
    <coach name="$Name" <<year="$Birthday+Year">> country="$Country"<< club="$Club">>/>
end

def type(Person)
    <player name="$Name" year="$Birthday+Year" country="$Country"<< club="$Club">>/>
end
